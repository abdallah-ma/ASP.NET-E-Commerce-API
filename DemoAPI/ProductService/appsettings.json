{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ServiceSettings":{
    "ServiceName": "ProductService"
  },

  "RabbitMQSettings":{
    "Host":"localhost"
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=DemoAPI;TrustServerCertificate=True;Trusted_Connection=True;MultipleActiveResultsets=True",
    "Redis": "localhost",
    "IdentityConnection": "Server=.;Database=Identity;TrustServerCertificate=True;Trusted_Connection=True;MultipleActiveResultsets=True"
  },

  "JWT": {
    "AuthKey": "ThisIsASecretKeyForJWTAuthentication",
    "Issuer": "https://localhost:5058",
    "DurationDays": "1"
  },

  "Kestrel": {
    "Endpoints": {

      
      "Grpc": {
        "Url": "http://localhost:6058",
        "Protocols": "Http2"
      },
      "Http": {
        "Url": "http://localhost:5058",
        "Protocols": "Http1"
      }
    }
  },



  "ApiBaseUrl": "https://localhost:5058/api"
}
