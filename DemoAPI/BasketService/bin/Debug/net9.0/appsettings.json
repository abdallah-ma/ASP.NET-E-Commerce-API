{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ServiceSettings":{
    "ServiceName": "BasketService"
  },

  "RabbitMQSettings":{
    "Host":"localhost"
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=DemoAPI;TrustServerCertificate=True;Trusted_Connection=True;MultipleActiveResultsets=True",
    "Redis": "localhost",
    "IdentityConnection": "Server=.;Database=Identity;TrustServerCertificate=True;Trusted_Connection=True;MultipleActiveResultsets=True"
  },

  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://localhost:6055",
        "Protocols": "Http2"
      }
      ,
      "Http": {
        "Url": "http://localhost:5055",
        "Protocols": "Http1"
      }
    }
  },

  "ApiBaseUrl": "http://localhost:5055/api"

}
